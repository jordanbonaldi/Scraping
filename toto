<script>
(function(){
(function() {(function(root,factory){"use strict";if(typeof define==="function"&&define.amd){define("et",factory)}else{root.et=factory()}})(this,function(){"use strict";var debug={level:0};var Trackables={experiment:"e",stage:"s",goal:"g",customGoal:"cg",goalWithValue:"gwv"};var TrackedOnClient=[];var requestInfo=function(){var index={};var el;var store="";function write(){var oldStore=store;store=Object.keys(index).join(",");if(el||(el=document.getElementById("req_info"))){if(el.innerHTML!==oldStore){updateIndex(el.innerHTML);store=Object.keys(index).join(",")}el.innerHTML=store}}function updateIndex(rawString){index=rawString.split(",").reduce(function(index,element){index[element]=true;return index},index)}function populate(item){index[item]=true}function api(obj){populate(obj);if(typeof obj==="string"){populate(obj);write()}else if(obj instanceof Array){obj.forEach(populate);write()}}return{populate:api}}();var url;var SENDING_DELAY=0;var FAIL_DELAY=100;var FAIL_ATTEMPTS=10;var sendBeacon=function(){var isSendingBeacon=false;var timeout;var payload=[];var currentlySending=[];var failCounter=0;function stringifyPayload(payload){var exps=[];var goals=[];var customGoals=[];var goalsWithValues=[];var stages=[];for(var tmp,i=0,l=payload.length;i<l;++i){tmp=payload[i];switch(tmp.what){case Trackables.experiment:exps.push(tmp.hash);break;case Trackables.stage:stages.push(tmp.hash+"|"+tmp.id);break;case Trackables.goal:goals.push(tmp.hash);break;case Trackables.customGoal:customGoals.push(tmp.hash+"|"+tmp.id);break;case Trackables.goalWithValue:var gwvStringified=gwvReport(tmp.hash);if(gwvStringified){goalsWithValues.push(gwvStringified)}break;default:debug.level&&debug.report(debug.events.UNABLE_TO_STRINGIFY,tmp)}}return"ete="+exps.join(",")+"&etg="+goals.join(",")+"&etcg="+customGoals.join(",")+"&ets="+stages.join(",")+"&etgwv="+goalsWithValues.join(",")}function ajax(options){var url=options.url;var callback=options.complete||function(){};var headers=options.headers||{};var header;var XHR_DONE_STATE=XMLHttpRequest.DONE||4;var httpRequest=new XMLHttpRequest;if(!url){return false}httpRequest.open("GET",url,true);if(headers){for(header in headers){if(headers.hasOwnProperty(header)){httpRequest.setRequestHeader(header,typeof headers[header]==="function"?headers[header].call():headers[header])}}}httpRequest.onreadystatechange=function onStateChange(){if(httpRequest.readyState===XHR_DONE_STATE){callback(httpRequest,httpRequest.status)}};httpRequest.send()}function onRequestSucceeded(){debug.level&&debug.report(debug.events.BEACON_SENT,payload);isSendingBeacon=false;failCounter=0;timeout=null;if(payload.length){sendRequest()}}function onRequestFailed(){failCounter++;isSendingBeacon=false;timeout=null;if(failCounter>=FAIL_ATTEMPTS){currentlySending=[]}else{payload=payload.concat(currentlySending);currentlySending=[];timeout=window.setTimeout(sendRequest,failCounter*FAIL_DELAY)}}function sendRequest(){debug.level&&debug.report(debug.events.SEND_BEACON,payload.slice(0));isSendingBeacon=true;currentlySending=payload;var requestUrl=url+"&"+stringifyPayload(currentlySending);if(jset.m){requestUrl+="&m="+encodeURIComponent(jset.m)}payload=[];try{ajax({url:requestUrl,complete:function(xhr,status){if(status===200){onRequestSucceeded()}else{onRequestFailed()}},headers:ajaxHeaders})}catch(e){var img=new Image;img.onload=onRequestSucceeded;img.onerror=onRequestFailed;img.src=url}}function send(what,hash,id){debug.level&&debug.report(debug.events.INIT_BEACON,what,hash,id);if(arguments.length===0){}payload.push({what:what,hash:hash,id:id});if(isSendingBeacon||timeout){debug.level&&debug.report(debug.events.DEFER_BEACON,payload)}else{timeout=window.setTimeout(sendRequest,SENDING_DELAY)}}return send}();var fnvCache={};var onViewHandlerDebounceTime=300;var onViewHandlerDelayed=false;var wasTracked={};var toBeTrackedOnView=[];var onViewHandlerAttached=false;var onViewHandlerRunning=false;var useFNV=false;var shouldNotTrack=false;var ajaxHeaders={};var bindCustomOnView=false;var unbindCustomOnView=false;var isDevServer=false;var jset={r:{},t:{},f:{}};if(!jset.r){jset.r={}}if(!jset.f){jset.f={}}if(!jset.t){jset.t={}}var gwvCache={};var GWV_VALUES_LIMIT_PER_PAGEVIEW=50;function gwvRegister(name,value){if(!gwvCache[name]){gwvCache[name]=[[],[]]}var newValues=gwvCache[name][0];var oldValues=gwvCache[name][1];if(oldValues.length<=GWV_VALUES_LIMIT_PER_PAGEVIEW+10){newValues.push(value);return true}}function gwvReport(name){if(!gwvCache[name]||!gwvCache[name][0].length){return}var newValues=gwvCache[name][0];var oldValues=gwvCache[name][1];var result=[name,newValues.join(":")];if(oldValues.length){result.push(oldValues.join(":"))}[].push.apply(oldValues,newValues.splice(0,newValues.length));return result.join("|")}function trackingIndexKey(what,hash,id){var res;if(what===Trackables.experiment||what===Trackables.goal){res=[what,hash]}else{res=[what,hash,id]}return res.join("-")}function fnvHash(hash){if(!useFNV){return hash}if(fnvCache[hash]){return fnvCache[hash]}var offset=2166136261;for(var index=0,length=hash.length;index<length;++index){offset+=(offset<<1)+(offset<<4)+(offset<<7)+(offset<<8)+(offset<<24);offset^=hash.charCodeAt(index)}return fnvCache[hash]=(offset>>>0).toString(16)}function genericTrack(what,hash,id){debug.level&&debug.report(debug.events.TRACKING_ATTEMPT,{what:what,hash:hash,id:id,variant:(what===Trackables.experiment||what===Trackables.stage)&&_variant(hash)});if(shouldTrack(what,hash,id)){switch(what){case Trackables.experiment:setTracked(Trackables.experiment,hash);requestInfo.populate(hash);if(jset.m){TrackedOnClient.push(hash)}sendBeacon(Trackables.experiment,hash);break;case Trackables.stage:setTracked(Trackables.stage,hash,id);requestInfo.populate(hash+"|"+id);if(jset.m){TrackedOnClient.push(hash+"|"+id)}sendBeacon(Trackables.stage,hash,id);break;case Trackables.goal:setTracked(Trackables.goal,hash);sendBeacon(Trackables.goal,hash);break;case Trackables.customGoal:setTracked(Trackables.customGoal,hash,id);sendBeacon(Trackables.customGoal,hash,id);break;case Trackables.goalWithValue:if(gwvRegister(hash,id)){sendBeacon(Trackables.goalWithValue,hash,id)}break;default:debug.level&&debug.report(debug.events.TRACK_UNKNOWN_ITEM,what,hash,id)}}if(what===Trackables.experiment){return _variant(hash)}else{return true}}function setTracked(what,hash,id){wasTracked[trackingIndexKey(what,hash,id)]=true}function shouldTrack(what,hash,id){if(shouldNotTrack){return false}debug.level&&debug.report(debug.events.SHOULD_TRACK,what,hash,id);var config={what:what,hash:hash,id:id,variant:(what===Trackables.experiment||what===Trackables.stage)&&_variant(hash)};var encodedHash;var bitNumber;if(wasTracked[trackingIndexKey(what,hash,id)]){debug.level&&debug.report(debug.events.NOT_TRACKING_WAS_TRACKED,config);return false}else{if(what===Trackables.experiment||what===Trackables.stage){bitNumber=1<<(id||0);encodedHash=fnvHash(hash);if(jset.f[encodedHash]){debug.level&&debug.report(debug.events.NOT_TRACKING_FULLON,config);return false}else if(jset.r[encodedHash]===undefined){debug.level&&debug.report(debug.events.NOT_TRACKING_NOT_RUNNING,config);return false}else if(jset.t[encodedHash]&bitNumber){setTracked(what,hash,id);debug.level&&debug.report(debug.events.NOT_TRACKING_WAS_TRACKED,config);return false}}else if(what===Trackables.customGoal){encodedHash=fnvHash(hash);if(jset.f[encodedHash]){debug.level&&debug.report(debug.events.NOT_TRACKING_FULLON,config);return false}else if(jset.r[encodedHash]===undefined){debug.level&&debug.report(debug.events.NOT_TRACKING_NOT_RUNNING,config);return false}}}return true}function setEventHandlers(eventName,elem,what,hash,id){debug.level&&debug.report(debug.events.ADD_EVENT_LISTENER,eventName,elem,what,hash,id);var elements=selectElements(elem);if(elements.length>0){if(eventName==="view"){trackOnView(elements[0],what,hash,id)}else{for(var i=0,l=elements.length;i<l;i++){addEvent(elements[i],eventName,fn)}}}else{debug.level&&debug.report(debug.events.NOT_EXISTING_ELEMENT_WONT_ADD_LISTENER,what,hash,id)}function fn(){genericTrack(what,hash,id);for(var i=0,l=elements.length;i<l;i++){removeEvent(elements[i],eventName,fn)}}}function onViewHandlerWindowLoad(){window.setTimeout(onViewHandler,onViewHandlerDebounceTime)}function trackOnView(element,what,hash,id){debug.level&&debug.report(debug.events.ADD_DEBOUNCED_VIEW_HANDLER,element,what,hash,id);var key=trackingIndexKey(what,hash,id);if(wasTracked[key]){return false}toBeTrackedOnView.push([element,what,hash,id]);if(!onViewHandlerAttached){debug.level&&debug.report(debug.events.ATTACH_VIEW_LISTENER,toBeTrackedOnView);addEvent(window,"scroll",onViewHandler);addEvent(window,"resize",onViewHandler);addEvent(window,"load",onViewHandlerWindowLoad);window.setTimeout(onViewHandler,onViewHandlerDebounceTime);if(bindCustomOnView){bindCustomOnView(onViewHandler)}onViewHandlerAttached=true}}function onViewHandler(){if(onViewHandlerRunning){if(!onViewHandlerDelayed){onViewHandlerDelayed=setTimeout(function(){onViewHandlerDelayed=false;onViewHandler()},onViewHandlerDebounceTime)}else{return}}onViewHandlerRunning=true;var tmp=[];debug.level&&debug.report(debug.events.CHECK_IF_VISIBLE,toBeTrackedOnView);var currentExp;for(var i=0,l=toBeTrackedOnView.length;i<l;++i){currentExp=toBeTrackedOnView[i];if(currentExp&&isVisible(currentExp[0])){debug.level&&debug.report(debug.events.ONVIEW_TRACKING_TRIGGERED,currentExp[1],currentExp[2],currentExp[3]);genericTrack(currentExp[1],currentExp[2],currentExp[3])}else{tmp.push(currentExp)}}toBeTrackedOnView=tmp;debug.level&&debug.report(debug.events.VISIBLE_CHECK_FINISHED,toBeTrackedOnView);if(toBeTrackedOnView.length===0){onViewHandlerAttached=false;removeEvent(window,"scroll",onViewHandler);removeEvent(window,"resize",onViewHandler);removeEvent(window,"load",onViewHandlerWindowLoad);if(unbindCustomOnView){unbindCustomOnView(onViewHandler)}debug.level&&debug.report(debug.events.DETACH_VIEW_LISTENER)}window.setTimeout(function(){onViewHandlerRunning=false},onViewHandlerDebounceTime)}function isVisible(element){var bounds;var clientViewPortHeight;var clientViewPortWidth;if(!element){return false}if(!element.parentElement){return false}if(!element.getBoundingClientRect){return true}bounds=element.getBoundingClientRect();clientViewPortHeight=window.innerHeight||document.documentElement.clientHeight;clientViewPortWidth=window.innerWidth||document.documentElement.clientWidth;if(bounds.right<0||bounds.left>clientViewPortWidth||bounds.top===0&&bounds.left===0&&bounds.right===0&&bounds.bottom===0){return false}return bounds.top<clientViewPortHeight}function selectElements(elem){if(typeof elem==="string"){return toArray(document.querySelectorAll(elem))}else if(elem instanceof HTMLCollection){return toArray(elem)}else if(elem instanceof NodeList){return toArray(elem)}else if(elem instanceof Element){return[elem]}else if(Object.prototype.toString.call(elem)==="[object Array]"){return elem}else if(window.jQuery&&elem instanceof jQuery){return elem.toArray()}return[]}function addEvent(obj,type,fn){if(obj.attachEvent){obj["e"+type+fn]=fn;obj[type+fn]=function(){obj["e"+type+fn](window.event)};obj.attachEvent("on"+type,obj[type+fn])}else{obj.addEventListener(type,fn,false)}}function removeEvent(obj,type,fn){if(obj.detachEvent){if(obj[type+fn]){obj.detachEvent("on"+type,obj[type+fn]);obj[type+fn]=null}}else{obj.removeEventListener(type,fn,false)}}function generateEventTracker(eventName,elem,what){return function(hash,id){if(shouldTrack(what,hash,id)){setEventHandlers(eventName,elem,what,hash,id)}else{debug.level&&debug.report(debug.events.WONT_ATTACH_EVENT_TRACKING,eventName,elem,what,hash,id)}}}function on(eventName,elem){return{track:generateEventTracker(eventName,elem,Trackables.experiment),stage:generateEventTracker(eventName,elem,Trackables.stage),goal:generateEventTracker(eventName,elem,Trackables.goal),customGoal:generateEventTracker(eventName,elem,Trackables.customGoal)}}function extend(dest,src){for(var p in src)if(src.hasOwnProperty(p)){dest[p]=src[p]}}function set(newJset){jset.f=newJset.f||{};extend(jset.r,newJset.r||{});extend(jset.t,newJset.t||{});if(newJset.m&&!jset.m){jset.m=newJset.m;TrackedOnClient=[]}}function _variant(rawHash){var hash=fnvHash(rawHash);return jset.r[hash]||jset.f[hash]||0}function track(hash){if(arguments.length>1){throw"Track only accept one parameter"}if(!hash){if(isDevServer){throw"B.et.track: hash value should be a non-empty string"}return 0}return genericTrack(Trackables.experiment,hash)}function trackStage(hash,num){if(!hash){if(isDevServer){throw"B.et.trackStage: hash value should be a non-empty string"}return false}if(num===undefined){if(isDevServer){throw"B.et.trackStage: stage number should be a number between 1 and 9"}return false}if(num===0){return track(hash)}if(!/^[1-9]$/.test(num)){if(isDevServer){throw"B.et.trackStage: stage number should be a number between 1 and 9"}return false}return genericTrack(Trackables.stage,hash,num)}function goal(name){if(!name){if(isDevServer){throw"B.et.goal: name should be a non-empty string"}return false}return genericTrack(Trackables.goal,name)}function customGoal(hash,num){if(!hash||!num||!/^[1-5]$/.test(num)){if(isDevServer){if(!hash){throw"B.et.customGoal: hash value should be a non-empty string"}else if(!num||!/^[1-5]$/.test(num)){throw"B.et.customGoal: custom goal number should be a number between 1 and 5"}}return false}return genericTrack(Trackables.customGoal,hash,num)}function goalWithValue(name,value){if(!/^js_/.test(name)||!/^-?[0-9]+$/.test(value)){if(isDevServer){if(!/^js_/.test(name)){throw"B.et.goalWithValue: name should be a non-empty string with prefix js_"}else if(!/^-?[0-9]+$/.test(value)){throw"B.et.goalWithValue: value should be an integer"}}return false}return genericTrack(Trackables.goalWithValue,name,value)}function initAttributesTracking(root){var i,event,attr,events=["change","click","mouseenter","touch","focus","view"];addDataAttributesTracking();function addDataAttributesTrac