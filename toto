  function clearFilters() {
    require(["babel/babel-helpers", 'trjs!common/track-filter'], function (babelHelpers, trackFilters) {
      trackFilters('overall_clear', 'unselect_link', 'clear_all', 0);
    });
    Filters.clearAll();
    wcBridgeRadio.trigger("externalClearAll");
    window.scrollTo(0, 0);
  }

  var clearAllElement = widget.element.querySelector(".clearAllFilters");
  if (clearAllElement) {
    clearAllElement.addEventListener("click", clearFilters);
  }
});
