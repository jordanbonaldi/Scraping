define(["widget", 'lib/jquery-amd', 'common/Radio'], function (widget, $, radio) {
  'use strict';

  var widgetElement = $(widget.element);
  var $button = widgetElement.find(".map_list_column");

  var mapRadio = radio('h-map');
  function toggleMap() {
    mapRadio && mapRadio.trigger('open-trigger-clicked'); // Open map
  }
  $button.on('click', toggleMap);

  return {
    toggleMap: toggleMap
  };
}.bind({}));
